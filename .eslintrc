{
    "env": {
        "browser": true,
        "es6": true
    },
    "plugins": ["import"],
    // "extends": ["eslint:recommended", "plugin:react/recommended"],
    // "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "parser": "@babel/eslint-parser",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-console": "warn",
        "import/first": "error"
    }
}